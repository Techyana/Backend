  "info": {
    "title": "Parts Management API",
    "description": "Endpoints to manage parts, claims, and users",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "access-token": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "UpdatePasswordDto": {
        "type": "object",
        "properties": {
          "passwordCurrent": {
            "type": "string",
            "example": "OldPassword123!"
          },
          "password": {
            "type": "string",
            "example": "NewPassword456!",
            "minLength": 8,
            "maxLength": 32,
            "description": "Must include uppercase, lowercase, number, and special character"
          },
          "passwordConfirm": {
            "type": "string",
            "example": "NewPassword456!"
          }
        },
        "required": [
          "password",
          "passwordConfirm"
        ]
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@example.com"
          },
          "password": {
            "type": "string",
            "example": "SuperSecret123!",
            "minLength": 8,
            "maxLength": 32
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "CreatePartDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "partNumber": {
            "type": "string"
          },
          "forDeviceModels": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "quantity": {
            "type": "number",
            "minimum": 0,
            "default": 0
          },
          "status": {
            "type": "string",
            "enum": [
              "AVAILABLE",
              "PENDING_COLLECTION",
              "CLAIMED",
              "COLLECTED",
              "UNAVAILABLE"
            ]
          }
        },
        "required": [
          "name",
          "partNumber",
          "forDeviceModels",
          "quantity"
        ]
      },
      "PartResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "partNumber": {
            "type": "string"
          },
          "forDeviceModels": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "quantity": {
            "type": "number"
          },
          "status": {
            "type": "string",
            "enum": [
              "AVAILABLE",
              "PENDING_COLLECTION",
              "CLAIMED",
              "COLLECTED",
              "UNAVAILABLE"
            ]
          },
          "claimedByName": {
            "type": "object"
          },
          "claimedAt": {
            "type": "string",
            "format": "date-time"
          },
          "createdAtTimestamp": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAtTimestamp": {
            "type": "string",
            "format": "date-time"
          }
        },
        "required": [
          "id",
          "name",
          "partNumber",
          "forDeviceModels",
          "quantity",
          "status",
          "createdAtTimestamp",
          "updatedAtTimestamp"
        ]
      },
      "ErrorResponseDto": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number",
            "example": 400,
            "description": "HTTP status code of the error"
          },
          "error": {
            "type": "string",
            "example": "Bad Request",
            "description": "Short description of the error type"
          },
          "message": {
            "example": [
              "username must be a string",
              "password should not be empty"
            ],
            "description": "Detailed error message(s); either a string or an array of strings",
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "timestamp": {
            "type": "string",
            "example": "2025-09-08T09:30:00.000Z",
            "description": "Timestamp of when the error occurred"
          },
          "path": {
            "type": "string",
            "example": "/auth/login",
            "description": "Request path that triggered the error"
          }
        },
        "required": [
          "statusCode",
          "error",
          "message",
          "timestamp",
          "path"
        ]
      },
      "UpdatePartDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "partNumber": {
            "type": "string"
          },
          "forDeviceModels": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "quantity": {
            "type": "number"
          },
          "status": {
            "type": "string",
            "enum": [
              "AVAILABLE",
              "PENDING_COLLECTION",
              "CLAIMED",
              "COLLECTED",
              "UNAVAILABLE"
            ]
          }
        }
      },
      "NotificationResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Notification UUID",
            "format": "uuid"
          },
          "userId": {
            "type": "string",
            "description": "UUID of the user receiving this notification",
            "format": "uuid"
          },
          "message": {
            "type": "string",
            "description": "Notification message text"
          },
          "type": {
            "type": "string",
            "description": "Category of notification",
            "enum": [
              "PART_AVAILABLE",
              "PART_REMOVED",
              "PART_CLAIMED",
              "PART_COLLECTED",
              "GENERAL"
            ],
            "example": "PART_AVAILABLE"
          },
          "isRead": {
            "type": "boolean",
            "description": "Read status",
            "default": false
          },
          "metadata": {
            "type": "object",
            "description": "Arbitrary metadata payload",
            "additionalProperties": true
          },
          "timestamp": {
            "format": "date-time",
            "type": "string",
            "description": "When this notification was created"
          }
        },
        "required": [
          "id",
          "userId",
          "message",
          "type",
          "isRead",
          "timestamp"
        ]
      },
      "CreateNotificationDto": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "format": "uuid"
          },
          "type": {
            "type": "string",
            "enum": [
              "PART_AVAILABLE",
              "PART_REMOVED",
              "PART_CLAIMED",
              "PART_COLLECTED",
              "GENERAL"
            ]
          },
          "message": {
            "type": "string",
            "maxLength": 500
          },
          "metadata": {
            "type": "object",
            "additionalProperties": true,
            "description": "Arbitrary metadata payload"
          },
          "isRead": {
            "type": "boolean",
            "default": false
          }
        },
        "required": [
          "userId",
          "type",
          "message"
        ]
      },
      "UpdateNotificationDto": {
        "type": "object",
        "properties": {
          "isRead": {
            "type": "boolean"
          },
          "message": {
            "type": "string",
            "maxLength": 500
          },
          "type": {
            "type": "string",
            "enum": [
              "PART_AVAILABLE",
              "PART_REMOVED",
              "PART_CLAIMED",
              "PART_COLLECTED",
              "GENERAL"
            ]
          },
          "metadata": {
            "type": "object",
            "additionalProperties": true,
            "description": "Arbitrary metadata payload"
          }
        }
      },
      "CreateTransactionDto": {
        "type": "object",
        "properties": {
          "partId": {
            "type": "string",
            "format": "uuid"
          },
          "userId": {
            "type": "string",
            "format": "uuid"
          },
          "type": {
            "type": "string",
            "enum": [
              "CLAIM",
              "REQUEST",
              "RETURN",
              "COLLECT",
              "ADD"
            ]
          },
          "quantityDelta": {
            "type": "number",
            "minimum": 1
          },
          "details": {
            "type": "string",
            "description": "Optional notes/details"
          }
        },
        "required": [
          "partId",
          "userId",
          "type",
          "quantityDelta"
        ]
      },
      "Part": {
        "type": "object",
        "properties": {}
      },
      "User": {
        "type": "object",
        "properties": {}
      },
      "TransactionResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "part": {
            "$ref": "#/components/schemas/Part"
          },
          "type": {
            "type": "string",
            "enum": [
              "CLAIM",
              "REQUEST",
              "RETURN",
              "COLLECT",
              "ADD"
            ]
          },
          "user": {
            "$ref": "#/components/schemas/User"
          },
          "quantityDelta": {
            "type": "number"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          }
        },
        "required": [
          "id",
          "part",
          "type",
          "user",
          "quantityDelta",
          "createdAt"
        ]
      }
    }
  }
