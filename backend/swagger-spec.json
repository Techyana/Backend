  "info": {
    "title": "Parts Management API",
    "description": "Endpoints to manage parts, claims, and users",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "access-token": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "UpdatePasswordDto": {
        "type": "object",
        "properties": {}
      },
      "LoginDto": {
        "type": "object",
        "properties": {}
      },
      "CreatePartDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Human-readable name of this part",
            "example": "Fuser Unit Assembly"
          },
          "partNumber": {
            "type": "string",
            "description": "Unique manufacturer part number",
            "example": "D232-4069"
          },
          "forDeviceModels": {
            "description": "List of device models this part is compatible with",
            "example": [
              "MP C3003",
              "MP C4504"
            ],
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "quantity": {
            "type": "number",
            "description": "Initial stock quantity for this part",
            "example": 10,
            "minimum": 1
          }
        },
        "required": [
          "name",
          "partNumber",
          "forDeviceModels",
          "quantity"
        ]
      },
      "PartResponseDto": {
        "type": "object",
        "properties": {}
      },
      "ErrorResponseDto": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number",
            "example": 400,
            "description": "HTTP status code of the error"
          },
          "error": {
            "type": "string",
            "example": "Bad Request",
            "description": "Short description of the error type"
          },
          "message": {
            "example": [
              "username must be a string",
              "password should not be empty"
            ],
            "description": "Detailed error message(s); either a string or an array of strings",
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "timestamp": {
            "type": "string",
            "example": "2025-09-08T09:30:00.000Z",
            "description": "Timestamp of when the error occurred"
          },
          "path": {
            "type": "string",
            "example": "/auth/login",
            "description": "Request path that triggered the error"
          }
        },
        "required": [
          "statusCode",
          "error",
          "message",
          "timestamp",
          "path"
        ]
      },
      "UpdatePartDto": {
        "type": "object",
        "properties": {}
      }
    }
  }
